{
    "identifier": "main",
    "permissions": [
        { "allow": ["$RESOURCE/**"], "identifier": "fs:scope" },
        "core:default",
        "core:webview:allow-set-webview-zoom",
        "core:webview:allow-create-webview-window",
        "core:window:allow-destroy",
        "core:window:allow-close",
        "dialog:default",
        "fs:default",
        "fs:allow-write-text-file",
        "fs:allow-remove",
        "fs:allow-copy-file",
        "fs:allow-exists",
        "fs:allow-mkdir",
        "os:default",
        "process:default",
        "clipboard-manager:allow-read-text",
        "clipboard-manager:allow-write-text",
        "updater:default",
        "log:default"
    ],
    "windows": ["main"]
}
