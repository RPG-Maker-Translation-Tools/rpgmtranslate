{
    "identifier": "settings",
    "permissions": [
        {
            "allow": [
                "$RESOURCE/**",
                "/usr/share/fonts/**/*",
                "C:/Windows/Fonts/**/*"
            ],
            "identifier": "fs:scope"
        },
        "core:default",
        "fs:default",
        "dialog:default",
        "os:default",
        "core:window:allow-destroy",
        "log:allow-log"
    ],
    "windows": ["settings"]
}
